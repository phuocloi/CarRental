@model IEnumerable<CarRentalWebService.Models.Review>

@{
    ViewBag.Title = "GetTop";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>GetTop</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Content)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stars)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Model.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Content)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stars)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.email)
                </td>
            </tr>
        }

    </table>